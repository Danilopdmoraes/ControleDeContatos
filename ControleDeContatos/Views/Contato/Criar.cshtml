<div class="text-center">

    <h1 class="display-4">Cadastrar contato</h1>

</div>

<form>
    <div class="mb-3">
        <label for="nome" class="form-label">Nome</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Digite o nome do contato">
    </div>    
    
    <div class="mb-3">
        <label for="email" class="form-label">E-Mail</label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Digite o e-mail do contato">
    </div>
    
    <div class="mb-3">
        <label for="whatsapp" class="form-label">Whatsapp</label>
        <input type="tel" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Digite o Whatsapp do contato">
    </div>

    <div class="d-grid gap-2">
        <button type="submit" class="btn btn-primary">Adicionar</button>
        <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
    </div>

</form>